	measure 'Resolution Efficiency %' = ```
			
			VAR TotalCases = COUNTROWS('SupportCases')
			VAR ClosedCases = 
			    CALCULATE(
			        COUNTROWS('SupportCases'),
			        FILTER('SupportCases', 'SupportCases'[Status] = "Closed")
			    )
			RETURN
			DIVIDE(ClosedCases, TotalCases, 0)
			
			
			```
		formatString: 0.00%;-0.00%;0.00%
		lineageTag: be5dbea3-6ac5-4cdb-8c09-817289aa61bb

	measure 'Same Day Resolution %' = ```
			
			VAR ClosedCases =
			    CALCULATE(
			        COUNTROWS('SupportCases'),
			        FILTER(
			            'SupportCases',
			            'SupportCases'[Status] = "Closed"
			        )
			    )
			
			VAR SameDayResolved =
			    CALCULATE(
			        COUNTROWS('SupportCases'),
			        FILTER(
			            'SupportCases',
			            'SupportCases'[Status] = "Closed" &&
			            NOT ISBLANK('SupportCases'[CreatedOn]) &&
			            NOT ISBLANK('SupportCases'[DeactivatedOn]) &&
			            DATEDIFF('SupportCases'[CreatedOn], 'SupportCases'[DeactivatedOn], DAY) = 0
			        )
			    )
			
			RETURN
			IF(
			    ClosedCases = 0,
			    BLANK(),
			    DIVIDE(SameDayResolved, ClosedCases)
			)
			
			```
		lineageTag: 099d5dd6-d535-480b-a0fa-65cd11cda34d

