			        'SupportCases',
			        NOT(ISBLANK('SupportCases'[FirstResponseBy]))
			            && 'SupportCases'[FirstResponseBy] >= 'SupportCases'[CreatedOn]
			    ),
			    DATEDIFF('SupportCases'[CreatedOn], 'SupportCases'[FirstResponseBy], HOUR)
