	measure 'TTR Bucket' = ```
			
			SWITCH(
			    TRUE(),
			    SELECTEDVALUE('SupportCases'[TTR]) > 30, "Over 30 days",
			    SELECTEDVALUE('SupportCases'[TTR]) <= 30, "30 or fewer days",
			    BLANK()
